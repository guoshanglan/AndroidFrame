<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="105dp" android:height="28dp" android:state_selected="false">
        <shape android:shape="rectangle">
            <corners android:radius="4dp" />
            <solid android:color="@color/custom_time_border_color_normal" />
        </shape>
    </item>
    <item android:width="105dp" android:height="28dp" android:state_selected="true">
        <shape android:shape="rectangle">
            <corners android:radius="4dp" />
            <solid android:color="@color/custom_time_border_color_selected" />
        </shape>
    </item>
</selector>