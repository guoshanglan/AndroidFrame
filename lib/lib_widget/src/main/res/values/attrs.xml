    <declare-styleable name="ZRHorizontalStepView">
        <attr name="count" format="integer" />
        <attr name="normal_point" format="reference" />
        <attr name="ongoing_point" format="reference" />
        <attr name="completed_point" format="reference" />
        <!--线条的宽度-->
        <attr name="line_width" format="dimension" />
        <!--未完成的线条的颜色-->
        <attr name="line_normal_color" format="color" />
        <!--已完成的线条的颜色-->
        <attr name="line_completed_color" format="color" />
        <!--说明文字的大小以及颜色-->
        <attr name="desc_textSize" format="dimension" />
        <attr name="desc_normal_textColor" format="color" />
        <attr name="desc_ongoing_textColor" format="color" />
        <attr name="desc_completed_textColor" format="color" />
        <!--步骤的进度条距离说明文字的距离-->
        <attr name="distance_from_text" format="dimension" />
        <!--普通的步骤点，进行中的步骤点和已经完成的步骤点的宽高-->
        <attr name="normal_point_width" format="dimension" />
        <attr name="ongoing_point_width" format="dimension" />
        <attr name="completed_point_width" format="dimension" />

        <!--文字处于上方或者下方：0下面，1上面-->
        <attr name="text_location" format="enum">
            <enum name="down" value="0" />
            <enum name="up" value="1" />
        </attr>
        <!--步骤项宽高的计算方式-->
        <attr name="stepWidthMode" format="enum">
            <!--根据View的宽度均分-->
            <enum name="average" value="0" />
            <!--自己设置的宽度，这种模式下，要设置步骤项的宽度，如果设置依旧是average-->
            <enum name="fixed" value="1" />
        </attr>
        <!--步骤项的宽度-->
        <attr name="step_width" format="dimension" />
    </declare-styleable>

